pipeline {
    agent any

    stages {
        stage('CHECKOUT'){
            steps {
                    git branch: 'main', credentialsId: 'key', url: 'https://github.com/Pradeep-hc/app.git'
            
            }
        }

        stage('BUILD'){
            steps {
               sh 'ls -lrt'
            }
        }

    stage('BUILD2'){
            steps {
               sh 'ls -lrt'
            }
        }
    }
}